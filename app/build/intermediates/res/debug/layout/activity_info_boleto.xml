<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >
	
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:orientation="vertical" >

    <TextView
        android:id="@+id/Tv_dt_vencimento"
        style="@style/layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="Data de vencimento : "
        android:textColor="#000000" />

    <TextView
        android:id="@+id/Tv_dt_vencimento2"
        style="@style/Exibicao_st"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:text="00/00/2013 "
        android:textColor="#000000" />

    <TextView
        android:id="@+id/Tv_dt_cadastro"
        style="@style/layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="Data de cadastro: "
        android:textColor="#000000" />

    <TextView
        android:id="@+id/Tv_dt_cadastro2"
        style="@style/Exibicao_st"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:text="00/00/2013 : "
        android:textColor="#000000" />

    <TextView
        android:id="@+id/fin"
        style="@style/layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="Financeira : "
        android:textColor="#000000" />
    
     <TextView
        android:id="@+id/fin2"
        style="@style/Exibicao_st"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:text="Banco Central "
        android:textColor="#000000" />
	</LinearLayout>
</RelativeLayout>
